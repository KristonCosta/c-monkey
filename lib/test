let map = fn(arr, f) {
  let iter = fn(arr, acc) {
    if (len(arr) == 0) {
      acc
    } else {
      iter(tail(arr), push(acc, f(head(arr))));
    }
  };
  iter(arr, []);
};

let reduce = fn(arr, initial, f) {
  let iter = fn(arr, res) {
    if (len(arr) == 0) {
      res
    } else {
      iter(tail(arr), f(res, head(arr)));
    }
  };
  iter(arr, initial);
};